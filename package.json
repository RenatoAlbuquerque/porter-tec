    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/material-nextjs": "^7.3.3",
    "@mui/x-data-grid": "^8.17.0",
    "@mui/x-date-pickers": "^8.14.0",
    "@tanstack/react-query": "^5.90.3",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "js-cookie": "^3.0.5",
    "next": "^15.5.5",
    "next-intl": "^4.3.12",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
